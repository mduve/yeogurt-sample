mixin breadcrumbs(spec)
  - spec = spec || {}
  - spec.__class = spec.__class || ''

  div(class=spec.__class)&attributes(attributes)
    nav.breadcrumb
      a.breadcrumb-item(href='#0') Home
      a.breadcrumb-item(href='#0') Library
      a.breadcrumb-item(href='#0') Data
      span.breadcrumb-item Current Page
